package cn.com.yajiaotong.wface.util;

/**
 * redis key常量
 * redis里面存储的所有的数据的key都的命名都统一写到这里面
 * @author meizhiwen
 *
 */
public class JK {

	public static final String MH=":";
	public static final String CAPTCHA="captcha";//验证码  有效时间为5分钟
	public static final String BLACKLIST="bl";//黑名单  key为bl:id value为手机号
	public static final String ID="id";//黑名单的自增值做id
	public static final String CREATEDATE="d";//创建时间
	public static final String PHONENUM="p";//号码
	
	public static final String BLACKDICT="bd";//黑名单  key为bd:id value为内容
	public static final String CONTENT="c";//content
	public static final String GENERATECOUNT = "gc";//手机号验证码生成的次数
	

	public static final String MODIFYDATE="md";//修改时间
	public static final String USERBLACKCONFIG="ubc";//用户黑名单配置
	public static final String USERBLACKLIST="ubl";//用户黑名单
	public static final String CHANNELID="cid";//通道Id
	public static final String USERID="uid";//用户ID
	public static final String TD="td";//TD内容
}
