package cn.com.yajiaotong.wface.util;


public class StringDo {	
	public static String Comma(String s){
		for(int i=0;i<2;i++){
			s=s.replace(",,", ",#,");
		}
		s=s.replace(",,", ",#,");
		if(s.startsWith(",")){
			s="#"+s;
		}
		if(s.endsWith(",")){
			s=s+"#";
		}
		return s;
	}
